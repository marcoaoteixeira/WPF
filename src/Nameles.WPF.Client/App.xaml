<Application x:Class="Nameless.WPF.Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             xmlns:converters="clr-namespace:Nameless.WPF.UI.Converters;assembly=Nameless.WPF.UI">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ui:ThemesDictionary Theme="Light" />
                <ui:ControlsDictionary />
                <ResourceDictionary Source="pack://application:,,,/Resources/ValidationResourceDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <converters:DateToStringValueConverter x:Key="DateToStringValueConverter" />
            <converters:TimeToStringValueConverter x:Key="TimeToStringValueConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>